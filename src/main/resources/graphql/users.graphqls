extend type Mutation {
    createUser(user: UserInput): Users
}

extend type Query {
    getUsers: [Users]
    findById(id: Long): Users
    users(page: PageQuery!): UsersConnection
}

input UserInput {
    username: String
}


type Users {
    id: Long
    username: String
    email: String
    password: String
    role: String
}